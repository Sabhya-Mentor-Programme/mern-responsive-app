    "node": "16.x",
    "yarn": "3.x",
    "npm": "8.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.0",
    "express": "^4.17.2",
    "express-rate-limit": "^6.0.5",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.5",
    "react-scripts": "4.0.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.15"
  },
  "packageManager": "yarn@3.1.1"
