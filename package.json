{
  "name": "theperception",
  "version": "1.0.0",
  "description": "TFI APP",
  "main": "server.js",
  "scripts": {
    "start": "node --max_old_space_size=4096 server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
